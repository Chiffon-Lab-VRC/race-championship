{
  "name": "race-championship",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:dev": "npx wrangler pages dev .vercel/output/static --compatibility-date=2024-09-23 --d1 DB=ebd79ae8-ba4a-4e97-b987-74dba1cd8c8d",
    "pages:deploy": "npm run pages:build && wrangler pages deploy .vercel/output/static",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply race-championship-db --local",
    "db:migrate:remote": "wrangler d1 migrations apply race-championship-db --remote",
    "db:seed:local": "wrangler d1 execute race-championship-db --local --file=scripts/seed-data.sql",
    "db:seed:remote": "wrangler d1 execute race-championship-db --remote --file=scripts/seed-data.sql"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.948.0",
    "drizzle-orm": "^0.45.1",
    "next": "^15.1.0",
    "react": "19.2.1",
    "react-dom": "19.2.1"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@cloudflare/workers-types": "^4.20251217.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "typescript": "^5",
    "wrangler": "^4.55.0"
  }
}